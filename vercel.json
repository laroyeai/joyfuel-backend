{
    "version": 2,
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/beta/register.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/beta/register",
            "dest": "api/beta/register.py"
        },
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11"
    }
} 